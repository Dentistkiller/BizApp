@model IEnumerable<BizApp.Models.Run>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.started_at)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.finished_at)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.model_version)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.label_policy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.notes)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.started_at)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.finished_at)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.model_version)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.label_policy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.notes)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.run_id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.run_id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.run_id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
