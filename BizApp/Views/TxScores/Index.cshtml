@model IEnumerable<BizApp.Models.TxScore>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.score)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.label_pred)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.threshold)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.reason_json)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.explained_at)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.run)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.tx)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.score)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.label_pred)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.threshold)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.reason_json)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.explained_at)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.run.run_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.tx.tx_id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.tx_id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.tx_id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.tx_id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
